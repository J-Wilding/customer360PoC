		{
			"name": "[concat(parameters('factoryName'), '/customer360_ETL')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "Addresses",
								"type": "DatasetReference"
							},
							"name": "Addresses"
						},
						{
							"dataset": {
