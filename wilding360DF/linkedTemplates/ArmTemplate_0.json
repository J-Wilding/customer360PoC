						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "Addresses_clean",
								"type": "DatasetReference"
							},
							"name": "AddressesClean"
						},
						{
							"dataset": {
								"referenceName": "Customers_clean",
								"type": "DatasetReference"
							},
							"name": "sink1"
						},
						{
							"dataset": {
								"referenceName": "Errors",
								"type": "DatasetReference"
							},
							"name": "sink2"
						}
					],
					"transformations": [
						{
							"name": "AddressesValidation"
						},
						{
							"name": "AddressesTransformation"
						},
						{
							"name": "FillNullAddressWithNA",
							"description": "address1, 2, 3 if NULL -> N/A"
						},
						{
							"name": "SelectAddresses"
						},
						{
							"name": "ConditionalSplit1"
						},
						{
							"name": "customerIDs"
						}
					],
					"script": "source(output(\n\t\taddress_id as long,\n\t\taddress_type as string,\n\t\taddress1 as string,\n\t\taddress2 as string,\n\t\taddress3 as string,\n\t\tcity as string,\n\t\tstate as string,\n\t\tcountry as string,\n\t\tzipcode as string,\n\t\tphone as string,\n\t\temail as string,\n\t\tFax as string,\n\t\tcustomer_id as long\n\t),\n\tallowSchemaDrift: true,\n\tvalidateSchema: false,\n\tignoreNoFilesFound: false) ~> Addresses\nsource(output(\n\t\tcity as string,\n\t\tstate as string,\n\t\tcountry as string\n\t),\n\tallowSchemaDrift: false,\n\tvalidateSchema: false,\n\tignoreNoFilesFound: false) ~> CityStateCountry\nsource(output(\n\t\tcustomer_id as long,\n\t\tsource_id as long,\n\t\tfirst_name as string,\n\t\tmiddle_name as string,\n\t\tlast_name as string,\n\t\tdob as string,\n\t\tgender as string,\n\t\tnationality as string,\n\t\tmarital_status as string,\n\t\toccupation as string,\n\t\tavg_family_size as string,\n\t\tincome_level as string,\n\t\tdate_onboarded as string,\n\t\tcreated_date as string,\n\t\tupdated_date as string,\n\t\teffective_start_date as string,\n\t\teffective_end_date as string,\n\t\tIdentifier_type as string,\n\t\tidentifier as string\n\t),\n\tallowSchemaDrift: true,\n\tvalidateSchema: false,\n\tignoreNoFilesFound: false) ~> Customers\nFillNullAddressWithNA, CityStateCountry, customerIDs assert(expectTrue(!isNull(address_id), false, 'addressIdNotNull', null, 'Address Id is null'),\n\texpectTrue(lower(address_type) == 'residence' || lower(address_type) == 'business', false, 'addressTypeValid', null, \"Address is not residence or business\"),\n\texpectExists(Addresses@city == CityStateCountry@city, false, 'cityExists', null, Addresses@city + ' does not exist in citystatecountry table'),\n\texpectExists(Addresses@state == CityStateCountry@state, false, 'stateExists', null, Addresses@city + ' does not exist in citystatecountry table'),\n\texpectExists(Addresses@country == CityStateCountry@country, false, 'countryExists', null, Addres